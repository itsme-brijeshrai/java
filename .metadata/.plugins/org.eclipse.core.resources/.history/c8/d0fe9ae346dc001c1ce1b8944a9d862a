package com.inheritance;

public class Super {
	String x = "Brijesh";

public static void main(String[] args) {
	thisInherit thisI = new thisInherit();
	thisI.call();
}	
		
}
class thisInherit extends Super{
	String x = "Anubhav";
	
	void call() {
		System.out.println(this.x);
		//this will print the explicit nearest x variable.
	}
}




